@model IEnumerable<engindemirog_blog.Models.Kategori>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout - Admin.cshtml";
}


<section id="main-content">
    <section class="wrapper">
       
        <!--overview start-->
        <div class="row">
            <div class="col-lg-12">
                @foreach (var item in Model)
                {
                    <li>@item.KategoriAd</li>
                }
                <h3 class="page-header"><i class="fa fa-laptop"></i> SİTE Genel Özet</h3>

            </div>
        </div>

        <h6>Burası Güncellenecek</h6>

    </section>
 
</section>

